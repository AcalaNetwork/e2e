{
  "name": "@acala-network/e2e",
  "version": "0.1.21",
  "main": "lib/index.js",
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org"
  },
  "resolutions": {
    "typescript": "4.0.5"
  },
  "bin": {
    "simulate-liquidate-cdp": "scripts/simulate-liquidate-cdp.js",
    "simulate-loan": "scripts/simulate-loan.js"
  },
  "scripts": {
    "build": "tsc",
    "test": "yarn jest --testPathIgnorePatterns e2e",
    "test:e2e": "yarn jest",
    "jest": "polkadot-dev-run-test --detectOpenHandles --runInBand --passWithNoTests --verbose",
    "lint": "polkadot-dev-run-lint",
    "build-and-publish": "scripts/build-and-publish",
    "dev:simulate-liquidate-cdp": "BIDDER_ADDRESS=5FLSigC9HGRKVhB9FiEo4Y3koPsNmBmLJbpXg2mp1hXcS59Y ts-node src/scripts/simulate-liquidate-cdp.ts",
    "dev:simulate-loan": "ADDRESS=5FLSigC9HGRKVhB9FiEo4Y3koPsNmBmLJbpXg2mp1hXcS59Y ts-node src/scripts/simulate-loan.ts"
  },
  "dependencies": {
    "@acala-network/api": "^3.0.2",
    "@open-web3/api": "^1.0.2-2",
    "@open-web3/util": "^1.0.2-2",
    "@polkadot/api": "^6.7.2",
    "@polkadot/dev": "^0.63.27",
    "@polkadot/keyring": "^7.8.2",
    "@polkadot/rpc-provider": "^6.7.2",
    "@polkadot/util": "^7.8.2",
    "@polkadot/util-crypto": "^7.8.2",
    "big.js": "^6.1.1",
    "dotenv": "^10.0.0"
  },
  "devDependencies": {
    "@open-web3/dev-config": "^0.1.13",
    "@types/big.js": "6.1.2",
    "@types/bn.js": "5.1.0",
    "@types/jest": "^27.0.2",
    "@types/node": "^16.11.7",
    "husky": "^7.0.4",
    "jest": "^27.3.1",
    "pretty-quick": "^3.1.1",
    "ts-node": "^10.4.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn pretty-quick --staged"
    }
  }
}
